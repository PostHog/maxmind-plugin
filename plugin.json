{
    "name": "posthog-maxmind-plugin",
    "url": "https://github.com/posthog/posthog-maxmind-plugin",
    "description": "Ingest GeoIP data via MaxMind",
    "main": "dist/index.js",
    "config": {
        "maxmindMmdb": {
            "name": "GeoIP .mddb database",
            "type": "attachment",
            "required": true
        },
        "localhostIP": {
            "name": "IP to use instead of 127.0.0.1",
            "type": "string",
            "default": "",
            "required": false
        }
    }
}
